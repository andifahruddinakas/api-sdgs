{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/cron.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "api/cron.js"
    },
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 0 1 * *"
    }
  ]
}